{
  "name": "rescript-project-template",
  "version": "0.0.1",
  "scripts": {
    "clean": "rescript clean -with-deps && rm -rf .parcel-cache",
    "build": "npm run clean && rescript build",
    "dev": "concurrently \"parcel ./public/index.html --open\" \"rescript build -w\" ",
    "start": "parcel ./public/index.html",
    "deploy": "vercel"
  },
  "keywords": [
    "rescript",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@rescript/react": "^0.10.3",
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.7",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "tailwindcss": "^3.0.23"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "parcel": "^2.3.2",
    "rescript": "9.1.2",
    "vercel": "^24.0.0"
  }
}
