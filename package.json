{
  "name": "rescript-project-template",
  "version": "0.0.1",
  "scripts": {
    "clean": "rescript clean -with-deps && rm -rf .parcel-cache",
    "build": "npm run clean && rescript build",
    "dev": "concurrently \"parcel ./public/index.html --open\" \"rescript build -w\" ",
    "start": "parcel ./public/index.html",
    "deploy": "vercel",
    "test": "jest",
    "compile": "tsc"
  },
  "keywords": [
    "rescript",
    "react"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@rescript/react": "^0.10.3",
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.7",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "tailwindcss": "^3.0.23"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "concurrently": "^7.0.0",
    "jest": "^27.5.1",
    "parcel": "^2.3.2",
    "rescript": "9.1.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2",
    "vercel": "^24.0.0"
  }
}
